{
    "name": "firecase",
    "version": "0.3.6",
    "description": "Rename the files in your projects to match known casings.",
    "main": "./lib/src/index.js",
    "scripts": {
        "build": "tsc",
        "start:prod": "cross-env NODE_ENV=prod node lib/src/index.js",
        "start:dev": "cross-env NODE_ENV=dev build && node lib/src/index.js",
        "dev": "cross-env NODE_ENV=dev nodemon src/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Cornayy/firecase.git"
    },
    "bin": {
        "firecase": "./lib/src/index.js"
    },
    "keywords": [
        "filecasing",
        "structure",
        "casing"
    ],
    "author": "Cornayy",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Cornayy/firecase/issues"
    },
    "homepage": "https://github.com/Cornayy/firecase#readme",
    "devDependencies": {
        "@types/lodash": "^4.14.157",
        "@types/node": "^14.0.14",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^3.5.0",
        "@typescript-eslint/parser": "^3.5.0",
        "eslint": "^7.3.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "nodemon": "^2.0.4",
        "prettier": "^2.0.5",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "cross-env": "^7.0.2",
        "lodash": "^4.17.15",
        "winston": "^3.3.3"
    }
}
